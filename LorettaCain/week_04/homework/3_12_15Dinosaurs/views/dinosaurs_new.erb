<h2>Add A Dinosaur</h2>

<form action="/dinosaurs" method="post"> 
<!-- Had to add method="post" to update form -->
  <label for="">Name:</label>
  <input type="text" name="name">
  <label for="">Eon:</label>
  <input type="text" name="eon">
  <label for="">Era:</label>
  <input type="text" name="era">
  <label for="">Period:</label>
  <input type="text" name="period">
  <label for="">Extent:</label>
  <input type="text" name="extent">
  <label for="">Duration:</label>
  <input type="text" name="duration">
  <label for="">Image:</label>
  <input type="url" name="image">

  <button>Add Dinosaur</button>
</form>

